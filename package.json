{
    "name": "flow-of-presence",
    "version": "1.0.0",
    "description": "workflow timer and tool",
    "main": "main.js",
    "type": "module",
    "scripts": {
        "start": "vite",
        "build": "vite build",
        "azoth:update": "npm run azoth:un && npm run azoth:i",
        "azoth:un": "npm un -D @azoth-web/vite-plugin-azoth && npm un @azoth-web/maat",
        "azoth:i": "npm i -D @azoth-web/vite-plugin-azoth && npm i @azoth-web/maat",
        "test": "exit 0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/martypdx/flow-of-presence.git"
    },
    "keywords": [
        "timer",
        "pomodoro",
        "presence",
        "work tracking",
        "tracking"
    ],
    "author": "Marty Nelson",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/martypdx/flow-of-presence/issues"
    },
    "homepage": "https://github.com/martypdx/flow-of-presence#readme",
    "devDependencies": {
        "@azoth-web/vite-plugin-azoth": "^1.4.0",
        "eslint": "^8.56.0",
        "vite": "^5.1.1"
    },
    "dependencies": {
        "@azoth-web/maat": "^1.4.0",
        "vite-plugin-inspect": "^0.8.3"
    }
}